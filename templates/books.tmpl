<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th>Title</th>
      <th>Author</th>
      <th>Description</th>
    </tr>
  </thead>
  <tbody>
  {{ range . }}
    <tr>
      <td>{{ .Title }}</td>
      <td>{{ .Author }}</td>
      <td>{{ .Description }}</td>
    </tr>
  {{ end }}
  </tbody>
</table>